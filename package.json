{
  "name": "renew-formalism",
  "description": "An abstract formalism to extend from",
  "version": "1.0.0",
  "scripts": {
    "dev": "rollup -cw",
    "karma-chrome": "karma start karma.conf.js --log-level debug --browsers Chrome",
    "karma-firefox": "karma start karma.conf.js --log-level debug --browsers Firefox",
    "karma-headless": "karma start karma.conf.js --log-level debug --browsers FirefoxHeadless",
    "lint": "eslint src --fix",
    "prebuild": "eslint src",
    "build": "rollup -c --environment BUILD:production"
  },
  "license": "MIT",
  "repository": "https://github.com/renew-js/renew-formalism",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "chai": "^4.2.0",
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-renew": "^1.0.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.0-rc.1",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "google",
      "renew"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
